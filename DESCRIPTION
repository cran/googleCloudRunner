        googlePubsubR (>= 0.0.2), httr (>= 1.4.1), jose (>= 1.0),
        jsonlite (>= 1.5), openssl (>= 1.4.1), plumber (>= 1.0.0),
        usethis (>= 1.6.0), utils, withr, yaml (>= 2.2.0)
